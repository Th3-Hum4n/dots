a df='df -h'
a du='du -h'
a mv='mv -iv'
a rm='rm -v'
a cp='cp -rv'
a ln='ln -sf'
a mkf='touch'
a mkd='mkdir -pv'
a free='free -h'
a rmf='rm -rf'
a fc-cache='fc-cache --verbose'
a grep='grep -i'
a su='su -'
a {sl,ls}=' ls -AF'
a reboot='doas reboot'
a poweroff='doas poweroff'
a shutdown='doas shutdown'
a datenow='date +"%a %d %b %Y %H:%M"'