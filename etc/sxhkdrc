Print
  $HOME/bin/screenshot

super + shift + Return
   st -g 80x40 -t floating-st

super + Print
  maim -u -d 1 > $HOME/var/pictures/scrots/$(date '+%a_%b_%d-%H:%M').png

super + v
  qutebrowser

super + shift + v
  notify-send "vol" "$($HOME/bin/vol -g)%"

super + f
  thunar

super + button3
  $HOME/bin/draw_st

super + ctrl + r
  $HOME/bin/record

super + r
  st -e 'ranger'

super + shift + r
  surf $HOME/opt/rustup/toolchains/stable-x86_64-unknown-linux-gnu/share/doc/rust/html/book/2018-edition/

super + u
  $HOME/bin/invcurwin

XF86AudioRaiseVolume
  $HOME/bin/vol -i 5%

XF86AudioLowerVolume
  $HOME/bin/vol -d 5%

XF86AudioMute
  $HOME/bin/vol -m

super + shift + k
  $HOME/bin/vol -i 1%

super + shift + j
  $HOME/bin/vol -d 1%

super + shift + m
  $HOME/bin/vol -m

super + shift + u
  $HOME/bin/vol -u

super + x
  $HOME/bin/turnoff

super + shift + v
  dash -c 'notify-send "current vol" `$HOME/bin/vol -g`'

super + shift + ctrl + m
  $HOME/bin/togmouse

alt + b
  $HOME/bin/restartbar

super + shift + w
  $HOME/bin/weather

super + shift + l
  sudo $HOME/bin/brness -i 1%

super + shift + h
  sudo $HOME/bin/brness -d 1%

super + w
  $HOME/bin/wcmv -u 20

super + a
  $HOME/bin/wcmv -l 20

super + s
  $HOME/bin/wcmv -d 20

super + d
  $HOME/bin/wcmv -r 20

