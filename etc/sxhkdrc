Print
  $HOME/bin/screenshot

super + shift + Return
   st -g 80x40 -t floating-st

super + Print
  maim -u -s > $HOME/var/pictures/scrots/$(date '+%a_%b_%d-%H:%M').png

super + v
  firefox

super + f
  thunar

super + button3
  $HOME/bin/draw_st

super + ctrl + r
  $HOME/bin/record

super + r
  st -e 'ranger'

super + shift + r
  surf $HOME/.rustup/toolchains/stable-x86_64-unknown-linux-gnu/share/doc/rust/html/book/2018-edition/

super + shift + u
  $HOME/bin/invcurwin

XF86AudioRaiseVolume
  $HOME/bin/vol -i 5%

XF86AudioLowerVolume
  $HOME/bin/vol -d 5%

XF86AudioMute
  $HOME/bin/vol -m

super + x
  $HOME/bin/turnoff  
